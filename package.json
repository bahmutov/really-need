    "wrapper"
  ],
  "license": "MIT",
  "main": "index.js",
  "pre-commit": [
    "npm test",
    "npm run test-spy",
    "npm version"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/really-need.git"
  },
  "scripts": {
    "build": "grunt",
